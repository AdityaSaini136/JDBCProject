package com.java.Arrays;

public class FindPrimeNum {
	public static void main(String[] args) {
       System.out.println(FIne the prime num);
		int  a = 3;
		int count =0;

		for(int i=0; i<a; i++) {
			if(a%i==0) {
				count++;
			}
			if(count==2) {
				System.out.println("Num is Prime.....");
			}else
				System.out.println("Num is Not Prime.....");
		}
	}
}