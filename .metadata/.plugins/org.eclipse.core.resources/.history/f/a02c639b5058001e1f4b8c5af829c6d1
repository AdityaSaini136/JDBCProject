package com.java.Arrays.LargestNum;

import java.util.ArrayList;
import java.util.Arrays;
import java.util.List;
import java.util.TreeSet;

import javax.swing.plaf.basic.BasicInternalFrameTitlePane.SizeAction;

public class Test {

	public static void main(String[] args) {
		List<Integer> list = null;
		Integer intArr [] =  {2,3,4,5,6,7,4,3,2,4,2};
		List<Integer> asList = Arrays.asList(intArr);
		TreeSet<Integer> ts = new TreeSet<>(asList);
		ArrayList<Integer> al = new ArrayList(ts);
		System.out.println(al.get(al.size()-2));
	}
}
