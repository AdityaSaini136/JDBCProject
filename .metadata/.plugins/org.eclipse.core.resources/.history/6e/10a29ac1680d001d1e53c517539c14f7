package com.hkcl;

public class Demo {

	public static void main(String[] args) {

		int intArray [] = new int [] {10,20,30,10,20};
		int temp=0;
		for(int i=0; i<intArray.length; i++) {
			for(int j=i+1; j<intArray.length; j++) {
				if(intArray[i] != intArray[j]) {
					temp = intArray[i];
				}
			}
		}
		System.out.println(temp);
	}
}